<?xml version='1.0' encoding='utf-8'?>
<widget id="com.wynmanagement.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>WYN Management</name>
    <description>
        Elite Talent Management - Scouting, Refining, Managing, Branding, and Publicity for exceptional talents.
    </description>
    <author email="contact@wynmanagement.com" href="https://wynmanagement.com">
        WYN Management Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    
    <!-- Android Configuration -->
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="res/android/icon/drawable-ldpi-icon.png" />
        <icon density="mdpi" src="res/android/icon/drawable-mdpi-icon.png" />
        <icon density="hdpi" src="res/android/icon/drawable-hdpi-icon.png" />
        <icon density="xhdpi" src="res/android/icon/drawable-xhdpi-icon.png" />
        <icon density="xxhdpi" src="res/android/icon/drawable-xxhdpi-icon.png" />
        <icon density="xxxhdpi" src="res/android/icon/drawable-xxxhdpi-icon.png" />
        <splash density="land-ldpi" src="res/android/splash/drawable-land-ldpi-screen.png" />
        <splash density="land-mdpi" src="res/android/splash/drawable-land-mdpi-screen.png" />
        <splash density="land-hdpi" src="res/android/splash/drawable-land-hdpi-screen.png" />
        <splash density="land-xhdpi" src="res/android/splash/drawable-land-xhdpi-screen.png" />
        <splash density="land-xxhdpi" src="res/android/splash/drawable-land-xxhdpi-screen.png" />
        <splash density="land-xxxhdpi" src="res/android/splash/drawable-land-xxxhdpi-screen.png" />
        <splash density="port-ldpi" src="res/android/splash/drawable-port-ldpi-screen.png" />
        <splash density="port-mdpi" src="res/android/splash/drawable-port-mdpi-screen.png" />
        <splash density="port-hdpi" src="res/android/splash/drawable-port-hdpi-screen.png" />
        <splash density="port-xhdpi" src="res/android/splash/drawable-port-xhdpi-screen.png" />
        <splash density="port-xxhdpi" src="res/android/splash/drawable-port-xxhdpi-screen.png" />
        <splash density="port-xxxhdpi" src="res/android/splash/drawable-port-xxxhdpi-screen.png" />
        <preference name="Orientation" value="portrait" />
        <preference name="android-minSdkVersion" value="22" />
        <preference name="android-targetSdkVersion" value="33" />
    </platform>
    
    <!-- iOS Configuration -->
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="57" platform="ios" src="res/ios/icon/icon.png" width="57" />
        <icon height="114" platform="ios" src="res/ios/icon/icon@2x.png" width="114" />
        <icon height="40" platform="ios" src="res/ios/icon/icon-40.png" width="40" />
        <icon height="80" platform="ios" src="res/ios/icon/icon-40@2x.png" width="80" />
        <icon height="120" platform="ios" src="res/ios/icon/icon-40@3x.png" width="120" />
        <icon height="50" platform="ios" src="res/ios/icon/icon-50.png" width="50" />
        <icon height="100" platform="ios" src="res/ios/icon/icon-50@2x.png" width="100" />
        <icon height="60" platform="ios" src="res/ios/icon/icon-60.png" width="60" />
        <icon height="120" platform="ios" src="res/ios/icon/icon-60@2x.png" width="120" />
        <icon height="180" platform="ios" src="res/ios/icon/icon-60@3x.png" width="180" />
        <icon height="72" platform="ios" src="res/ios/icon/icon-72.png" width="72" />
        <icon height="144" platform="ios" src="res/ios/icon/icon-72@2x.png" width="144" />
        <icon height="76" platform="ios" src="res/ios/icon/icon-76.png" width="76" />
        <icon height="152" platform="ios" src="res/ios/icon/icon-76@2x.png" width="152" />
        <icon height="167" platform="ios" src="res/ios/icon/icon-83.5@2x.png" width="167" />
        <icon height="29" platform="ios" src="res/ios/icon/icon-small.png" width="29" />
        <icon height="58" platform="ios" src="res/ios/icon/icon-small@2x.png" width="58" />
        <icon height="87" platform="ios" src="res/ios/icon/icon-small@3x.png" width="87" />
        <splash height="1136" platform="ios" src="res/ios/splash/Default-568h@2x~iphone.png" width="640" />
        <splash height="1334" platform="ios" src="res/ios/splash/Default-667h.png" width="750" />
        <splash height="2208" platform="ios" src="res/ios/splash/Default-736h.png" width="1242" />
        <splash height="1242" platform="ios" src="res/ios/splash/Default-Landscape-736h.png" width="2208" />
        <splash height="1536" platform="ios" src="res/ios/splash/Default-Landscape@2x~ipad.png" width="2048" />
        <splash height="2048" platform="ios" src="res/ios/splash/Default-Landscape@~ipadpro.png" width="2732" />
        <splash height="768" platform="ios" src="res/ios/splash/Default-Landscape~ipad.png" width="1024" />
        <splash height="2048" platform="ios" src="res/ios/splash/Default-Portrait@2x~ipad.png" width="1536" />
        <splash height="2732" platform="ios" src="res/ios/splash/Default-Portrait@~ipadpro.png" width="2048" />
        <splash height="1024" platform="ios" src="res/ios/splash/Default-Portrait~ipad.png" width="768" />
        <splash height="960" platform="ios" src="res/ios/splash/Default@2x~iphone.png" width="640" />
        <splash height="480" platform="ios" src="res/ios/splash/Default~iphone.png" width="320" />
        <splash height="2732" platform="ios" src="res/ios/splash/Default@2x~universal~anyany.png" width="2732" />
        <preference name="Orientation" value="portrait" />
    </platform>
    
    <!-- Plugins -->
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-statusbar" spec="2" />
    <plugin name="cordova-plugin-device" spec="2" />
    <plugin name="cordova-plugin-splashscreen" spec="5" />
    <plugin name="cordova-plugin-inappbrowser" spec="4" />
    <plugin name="cordova-plugin-network-information" spec="2" />
    
    <!-- Preferences -->
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="22" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="FadeSplashScreenDuration" value="300" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />
</widget>
